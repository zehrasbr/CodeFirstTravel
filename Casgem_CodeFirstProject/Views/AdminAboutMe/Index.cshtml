@model List<Casgem_CodeFirstProject.DAL.Entities.AboutMe>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımda</h2>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutMeID</td>
            <td>@item.Title1</td>
            <td><a class="btn btn-success" href="/AdminAboutMe/AdminAboutMeUpdate/@item.AboutMeID">Güncelle</a></td>
        </tr>
    }

</table>

